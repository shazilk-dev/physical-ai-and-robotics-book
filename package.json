{
  "name": "physical-ai-humanoids-textbook",
  "version": "1.0.0",
  "description": "Interactive textbook on Physical AI & Humanoid Robotics with RAG chatbot",
  "main": "index.js",
  "scripts": {
    "dev": "cd frontend && npm run start",
    "build": "cd frontend && npm run build",
    "serve": "cd frontend && npm run serve",
    "backend": "cd backend && uvicorn app.main:app --reload",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "test:backend": "cd backend && pytest",
    "seed:vector-db": "python scripts/rag/seed-vector-db.py"
  },
  "keywords": [
    "robotics",
    "physical-ai",
    "humanoid",
    "education",
    "ros2",
    "isaac-sim",
    "rag",
    "chatbot"
